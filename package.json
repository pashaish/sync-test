{
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
    "dotenv": "^16.0.3",
    "lodash": "^4.17.21",
    "log4js": "^6.9.1",
    "mongodb": "^5.3.0",
    "tsyringe": "^4.7.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.194",
    "prettier": "^2.8.8",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "name": "fundraiseup-backend-test",
  "description": "Данный модуль приложения генерирует фековых клиентов при помощи библиотеки faker и сохраняет их в монгу.",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev:app": "LOGGER_LEVEL=debug NODE_PATH=./src node -r ts-node/register ./src/app/index.ts",
    "dev:sync": "LOGGER_LEVEL=debug NODE_PATH=./src node -r ts-node/register ./src/sync/index.ts",
    "dev:sync-full": "LOGGER_LEVEL=debug NODE_PATH=./src node -r ts-node/register ./src/sync/index.ts --full-reindex",
    "format": "prettier --write ./",
    "build": "tsc",
    "prod:app": "NODE_PATH=./build node ./build/app/index.js",
    "prod:sync": "NODE_PATH=./build node ./build/sync/index.js",
    "prod:sync-full": "NODE_PATH=./build node ./build/sync/index.js --full-reindex"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
